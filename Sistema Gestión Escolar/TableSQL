-- Table: public.alumno

-- DROP TABLE public.alumno;

CREATE TABLE public.alumno
(
    nombre character varying COLLATE pg_catalog."default",
    apellido character varying COLLATE pg_catalog."default",
    numerocedula character varying COLLATE pg_catalog."default",
    contacto integer,
    "país" integer,
    direccion boolean DEFAULT true,
    curso boolean DEFAULT true,
    encargado boolean DEFAULT true
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.alumno
    OWNER to postgres;
    
    
-- Table: public.asistencia_curso

-- DROP TABLE public.asistencia_curso;

CREATE TABLE public.asistencia_curso
(
    empleado character varying COLLATE pg_catalog."default",
    fecha character varying COLLATE pg_catalog."default",
    curso character varying COLLATE pg_catalog."default",
    alumno character varying COLLATE pg_catalog."default",
    descripcion character varying COLLATE pg_catalog."default",
    justificativo character varying COLLATE pg_catalog."default"
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.asistencia_curso
    OWNER to postgres;
    
    
    
-- Table: public.contacto

-- DROP TABLE public.contacto;

CREATE TABLE public.contacto
(
    telefono character varying COLLATE pg_catalog."default",
    correoelectronico character varying COLLATE pg_catalog."default",
    celular character varying COLLATE pg_catalog."default"
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.contacto
    OWNER to postgres;
    
 
